{
  "name": "blackboard",
  "type": "dark",
  "colors": {
    "focusBorder": "#58805c",
    "editor.background": "#242d25",
    "editor.foreground": "#CEC0AB",
    "editor.lineHighlightBorder": "#2A352A",
    "editor.lineHighlightBackground": "#2A352A",
    "titleBar.activeBackground": "#1a201b",
    "titleBar.inactiveBackground": "#1a201b",
    "tab.activeBackground": "#242d25",
    "tab.inactiveBackground": "#1a201b",
    "tab.hoverBackground": "#2e3b30",
    "activityBar.background": "#1a201b",
    "statusBar.background": "#1a201b",
    "panel.background": "#1a201b",
    "editorGroup.emptyBackground": "#1a201b",
    "editorGroupHeader.tabsBackground": "#1a201b",
    "sideBarTitle.foreground": "#bbbbbb",
    "sideBar.background": "#202721",
    "menu.background": "#202721",
    "menu.selectionBorder": "#2A352A",
    "menu.selectionBackground": "#2A352A",
    "sideBarSectionHeader.background": "#2A352A",
    "button.background": "#38493b",
    "list.focusBackground": "#2A352A",
    "list.activeSelectionBackground": "#2A352A",
    "list.inactiveSelectionBackground": "#2A352Aa0",
    "breadcrumb.background": "#242d25",
    "input.background": "#1a201b",
    "pickerGroup.foreground": "#80a386",
    "peekViewResult.background": "#1a201b",
    "peekView.border": "#80a386",
    "peekViewResult.selectionBackground": "#2A352A",
    "peekViewEditor.background": "#242d25",
    "notificationCenterHeader.background": "#1a201b",
    "activityBarBadge.background": "#2c462c",
    "editorSuggestWidget.background": "#1a201b",
    "editorSuggestWidget.highlightForeground": "#80a386",
    "list.highlightForeground": "#80a386"
  },
  "tokenColors": [
    {
      "scope": ["meta.embedded", "source.groovy.embedded"],
      "settings": {
        "foreground": "#CEC0AB"
      }
    },
    {
      "scope": "emphasis",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": "strong",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "scope": "header",
      "settings": {
        "foreground": "#000080"
      }
    },
    {
      "scope": "comment",
      "settings": {
        "foreground": "#75A377"
      }
    },
    {
      "scope": "constant.language",
      "settings": {
        "foreground": "#DDDDDD"
      }
    },
    {
      "scope": ["constant.numeric"],
      "settings": {
        "foreground": "#BCF2CE"
      }
    },
    {
      "scope": "constant.regexp",
      "settings": {
        "foreground": "#646695"
      }
    },
    {
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#DDDDDD"
      }
    },
    {
      "scope": "entity.name.tag.css",
      "settings": {
        "foreground": "#d7ba7d"
      }
    },
    {
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#CEC0AB"
      }
    },
    {
      "scope": [
        "entity.other.attribute-name.class.css",
        "entity.other.attribute-name.class.mixin.css",
        "entity.other.attribute-name.id.css",
        "entity.other.attribute-name.parent-selector.css",
        "entity.other.attribute-name.pseudo-class.css",
        "entity.other.attribute-name.pseudo-element.css",
        "source.css.less entity.other.attribute-name.id",
        "entity.other.attribute-name.attribute.scss",
        "entity.other.attribute-name.scss"
      ],
      "settings": {
        "foreground": "#d7ba7d"
      }
    },
    {
      "scope": "invalid",
      "settings": {
        "foreground": "#f44747"
      }
    },
    {
      "scope": "markup.underline",
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "scope": "markup.bold",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#DDDDDD"
      }
    },
    {
      "scope": "markup.heading",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#DDDDDD"
      }
    },
    {
      "scope": "markup.italic",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": "markup.inserted",
      "settings": {
        "foreground": "#BCF2CE"
      }
    },
    {
      "scope": "markup.deleted",
      "settings": {
        "foreground": "#BCF2CE"
      }
    },
    {
      "scope": "markup.changed",
      "settings": {
        "foreground": "#DDDDDD"
      }
    },
    {
      "scope": "punctuation.definition.quote.begin.markdown",
      "settings": {
        "foreground": "#75A377"
      }
    },
    {
      "scope": "punctuation.definition.list.begin.markdown",
      "settings": {
        "foreground": "#6796e6"
      }
    },
    {
      "scope": "markup.inline.raw",
      "settings": {
        "foreground": "#BCF2CE"
      }
    },
    {
      "name": "brackets of XML/HTML tags",
      "scope": "punctuation.definition.tag",
      "settings": {
        "foreground": "#808080"
      }
    },
    {
      "scope": "meta.preprocessor",
      "settings": {
        "foreground": "#DDDDDD"
      }
    },
    {
      "scope": "meta.preprocessor.string",
      "settings": {
        "foreground": "#BCF2CE"
      }
    },
    {
      "scope": "meta.preprocessor.numeric",
      "settings": {
        "foreground": "#BCF2CE"
      }
    },
    {
      "scope": "meta.structure.dictionary.key.python",
      "settings": {
        "foreground": "#CEC0AB"
      }
    },
    {
      "scope": "meta.diff.header",
      "settings": {
        "foreground": "#DDDDDD"
      }
    },
    {
      "scope": "storage",
      "settings": {
        "foreground": "#DDDDDD"
      }
    },
    {
      "scope": "storage.type",
      "settings": {
        "foreground": "#DDDDDD"
      }
    },
    {
      "scope": "storage.modifier",
      "settings": {
        "foreground": "#DDDDDD"
      }
    },
    {
      "scope": "string",
      "settings": {
        "foreground": "#BCF2CE"
      }
    },
    {
      "scope": "string.tag",
      "settings": {
        "foreground": "#BCF2CE"
      }
    },
    {
      "scope": "string.value",
      "settings": {
        "foreground": "#BCF2CE"
      }
    },
    {
      "scope": "string.regexp",
      "settings": {
        "foreground": "#9dc5aa"
      }
    },
    {
      "name": "String interpolation",
      "scope": [
        "punctuation.definition.template-expression.begin",
        "punctuation.definition.template-expression.end",
        "punctuation.section.embedded"
      ],
      "settings": {
        "foreground": "#DDDDDD"
      }
    },
    {
      "name": "Reset JavaScript string interpolation expression",
      "scope": ["meta.template.expression"],
      "settings": {
        "foreground": "#CEC0AB"
      }
    },
    {
      "scope": [
        "support.type.vendored.property-name",
        "support.type.property-name",
        "variable.css",
        "variable.scss",
        "variable.other.less",
        "source.coffee.embedded"
      ],
      "settings": {
        "foreground": "#CEC0AB"
      }
    },
    {
      "scope": "keyword",
      "settings": {
        "foreground": "#DDDDDD"
      }
    },
    {
      "scope": "keyword.control",
      "settings": {
        "foreground": "#DDDDDD"
      }
    },
    {
      "scope": [
        "keyword.operator",
        "keyword.operator.new",
        "keyword.operator.expression",
        "keyword.operator.cast",
        "keyword.operator.sizeof",
        "keyword.operator.instanceof",
        "keyword.operator.logical.python"
      ],
      "settings": {
        "foreground": "#B2A795"
      }
    },
    {
      "scope": "keyword.other.unit",
      "settings": {
        "foreground": "#BCF2CE"
      }
    },
    {
      "scope": [
        "punctuation.section.embedded.begin.php",
        "punctuation.section.embedded.end.php"
      ],
      "settings": {
        "foreground": "#DDDDDD"
      }
    },
    {
      "scope": "support.function.git-rebase",
      "settings": {
        "foreground": "#CEC0AB"
      }
    },
    {
      "scope": "constant.sha.git-rebase",
      "settings": {
        "foreground": "#BCF2CE"
      }
    },
    {
      "name": "coloring of the Java import and package identifiers",
      "scope": [
        "storage.modifier.import.java",
        "variable.language.wildcard.java",
        "storage.modifier.package.java"
      ],
      "settings": {
        "foreground": "#CEC0AB"
      }
    },
    {
      "name": "this.self",
      "scope": "variable.language",
      "settings": {
        "foreground": "#ACEFAE"
      }
    },
    {
      "name": "Function declarations",
      "scope": [
        "entity.name.function",
        "support.function",
        "support.constant.handlebars"
      ],
      "settings": {
        "foreground": "#CEC0AB"
      }
    },
    {
      "name": "Types declaration and references",
      "scope": [
        "meta.return-type",
        "support.class",
        "support.type",
        "entity.name.type",
        "entity.name.class",
        "storage.type.numeric.go",
        "storage.type.byte.go",
        "storage.type.boolean.go",
        "storage.type.string.go",
        "storage.type.uintptr.go",
        "storage.type.error.go",
        "storage.type.rune.go",
        "storage.type.cs",
        "storage.type.generic.cs",
        "storage.type.modifier.cs",
        "storage.type.variable.cs",
        "storage.type.annotation.java",
        "storage.type.generic.java",
        "storage.type.java",
        "storage.type.object.array.java",
        "storage.type.primitive.array.java",
        "storage.type.primitive.java",
        "storage.type.token.java",
        "storage.type.groovy",
        "storage.type.annotation.groovy",
        "storage.type.parameters.groovy",
        "storage.type.generic.groovy",
        "storage.type.object.array.groovy",
        "storage.type.primitive.array.groovy",
        "storage.type.primitive.groovy"
      ],
      "settings": {
        "foreground": "#ABDBB7"
      }
    },
    {
      "name": "Types declaration and references, TS grammar specific",
      "scope": [
        "meta.type.cast.expr",
        "meta.type.new.expr",
        "support.constant.math",
        "support.constant.dom",
        "support.constant.json",
        "entity.other.inherited-class"
      ],
      "settings": {
        "foreground": "#ABDBB7"
      }
    },
    {
      "name": "Control flow keywords",
      "scope": "keyword.control",
      "settings": {
        "foreground": "#DDDDDD"
      }
    },
    {
      "name": "Variable and parameter name",
      "scope": [
        "variable",
        "meta.definition.variable.name",
        "support.variable",
        "entity.name.variable"
      ],
      "settings": {
        "foreground": "#CEC0AB"
      }
    },
    {
      "name": "Object keys, TS grammar specific",
      "scope": ["meta.object-literal.key"],
      "settings": {
        "foreground": "#CEC0AB"
      }
    },
    {
      "name": "CSS property value",
      "scope": [
        "support.constant.property-value",
        "support.constant.font-name",
        "support.constant.media-type",
        "support.constant.media",
        "constant.other.color.rgb-value",
        "constant.other.rgb-value",
        "support.constant.color"
      ],
      "settings": {
        "foreground": "#BCF2CE"
      }
    },
    {
      "name": "Regular expression groups",
      "scope": [
        "punctuation.definition.group.regexp",
        "punctuation.definition.group.assertion.regexp",
        "punctuation.definition.character-class.regexp",
        "punctuation.character.set.begin.regexp",
        "punctuation.character.set.end.regexp",
        "keyword.operator.negation.regexp",
        "support.other.parenthesis.regexp"
      ],
      "settings": {
        "foreground": "#BCF2CE"
      }
    },
    {
      "scope": [
        "constant.character.character-class.regexp",
        "constant.other.character-class.set.regexp",
        "constant.other.character-class.regexp",
        "constant.character.set.regexp"
      ],
      "settings": {
        "foreground": "#9dc5aa"
      }
    },
    {
      "scope": ["keyword.operator.or.regexp", "keyword.control.anchor.regexp"],
      "settings": {
        "foreground": "#CEC0AB"
      }
    },
    {
      "scope": "keyword.operator.quantifier.regexp",
      "settings": {
        "foreground": "#d7ba7d"
      }
    },
    {
      "scope": "constant.character",
      "settings": {
        "foreground": "#DDDDDD"
      }
    },
    {
      "scope": "constant.character.escape",
      "settings": {
        "foreground": "#b3ffd0"
      }
    },
    {
      "scope": "token.info-token",
      "settings": {
        "foreground": "#6796e6"
      }
    },
    {
      "scope": "token.warn-token",
      "settings": {
        "foreground": "#cd9731"
      }
    },
    {
      "scope": "token.error-token",
      "settings": {
        "foreground": "#f44747"
      }
    },
    {
      "scope": "token.debug-token",
      "settings": {
        "foreground": "#b267e6"
      }
    }
  ]
}
